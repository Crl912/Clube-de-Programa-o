
import random
num = random.randint(0,10)

while 1:
    
    palpite = input('Digite um numero')
    palpite = int(palpite)
    
    if (palpite <=0 or palpite > 100):
        print('numero invalido')
    elif palpite == num:
        print('voce venceu')
        break
    elif palpite < num:
        print('numero maior')
    elif palpite > num:
        print('numero menor')
        
